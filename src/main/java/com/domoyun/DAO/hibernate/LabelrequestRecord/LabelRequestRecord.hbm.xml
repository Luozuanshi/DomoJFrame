		</id>
		<!-- 建立普通属性与数据库表字段映射 -->
		 <property name="OrderID"/>
		 <property name="ChannelName"/>
		 <property name="ProductName"/>
		 <property name="SerialPackageNumber" />
		 <property name="TrackingNumber" />
		 <property name="DeliveryMethod" />
		 <property name="Weight"/>
		 <property name="WeightUnit"/>
		 <property name="Length"/>
		 <property name="Width"/>
		 <property name="Height"/>
		 <property name="SizeUnit"/>
		 <property name="ShipFirstName"/>
		 <property name="ShipLastName"/>
		 <property name="ShipCompany"/>
		 <property name="ShipStreetAddress"/>
		 <property name="ShipStreetAddress2"/>
		 <property name="ShipCity"/>
		 <property name="ShipState"/>
		 <property name="ShipZIPCode"/>
		 <property name="ShipCountry"/>
		 <property name="ShipPhoneNumber"/>
		 <property name="ShipIsResidential"/>
		 <property name="ShipToFirstName"/>
		 <property name="ShipToLastName"/>
		 <property name="ShipToCompany"/>
		 <property name="ShipToStreetAddress"/>
		 <property name="ShipToStreetAddress2"/>
		 <property name="ShipToStreetAddress3"/>
		 <property name="ShipToCity"/>
		 <property name="ShipToState"/>
		 <property name="ShipToZIPCode"/>
		 <property name="ShipToCountry"/>
		 <property name="ShipToPhoneNumber"/>
		 <property name="ShipToIsResidential"/>
		 <property name="CreateDate"/>
		 <property name="Created"/>
		 <property name="PackageNumber"/>
		 <property name="LabelStatus"/>
		 <property name="SyncFailCount"/>
		 <property name="SyncStatus"/>
		 <property name="PrintLabelType"/>
		 <property name="ChannelAccountInfoID"/>
		 <property name="IsMultiPrint"/>
		 <property name="RedundancyField"/>
		 <property name="MainID"/>
		 <property name="BoxNum"/>
		 <property name="LabelType"/>
		 <property name="timestamp"/>
		 <property name="LabelAccountId"/>
		 <property name="WayBillNumber"/>
		 <property name="ShipToPhoneExtension"/>
		 <property name="WarehouseCode"/>
	</class>
	
</hibernate-mapping>